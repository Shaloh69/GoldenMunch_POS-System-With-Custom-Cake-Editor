{
  "name": "goldenmunch-kiosk-web",
  "version": "1.0.0",
  "description": "GoldenMunch POS Kiosk Web Application - Next.js UI",
  "author": "GoldenMunch",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3002",
