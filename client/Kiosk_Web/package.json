    "@react-types/shared": "3.30.0",
    "@tailwindcss/postcss": "4.1.11",
    "@types/node": "^20.5.7",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "8.34.1",
